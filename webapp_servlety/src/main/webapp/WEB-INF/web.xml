<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcom-file>index.jsp</welcom-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>simpleServletXml</servlet-name>
    <servlet-class>Servlets.SimpleServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>simpleServletXml</servlet-name>
    <url-pattern>/simpleServletXml</url-pattern>
    <url-pattern>/simpleServletXml2</url-pattern>
  </servlet-mapping>

</web-app>
